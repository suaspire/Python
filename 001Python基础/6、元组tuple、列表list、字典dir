tuple = (4,3,2,1)
for content in tuple:
    print(content)

list = [1,2,3,4]
list.insert(1,0)
# for item in list:
#     print(item)
list.sort(reverse=True)
print(list)

multi_list = [
    [1,2,3],
    [2,3,4],
    [7,8,9]
]
for item in multi_list:
    for value in item:
        print(value)

dir = {'a':1,'b':2,"c":3}
for item in dir:
    print(item,':',dir[item])
