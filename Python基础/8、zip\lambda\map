a=[1,2,3]
b=[4,5,6]
print('a',a)
print('b',b)
print('zip(a,b)',zip(a,b))
print('list(zip(a,b))',list(zip(a,b)))
for i,j in zip(a,b):
    print((i,j))


fun = lambda x,y:x+y
print(fun(2,3))

def fun1(x,y):
    return x+y
#print(list(map(fun1,1,2)))
print(list(map(fun1,[1],[2])))
print(list(map(fun1,[1,3],[2,5])))
